[Unit]
Description=Mindbridge-queue-service service
After=network.target

[Service]
Restart=always
EnvironmentFile=/home/ubuntu/mindbridge_queue_service/env/mindbridge.env
User=ubuntu
Group=ubuntu
WorkingDirectory=/home/ubuntu/mindbridge_queue_service
ExecStart=/home/ubuntu/mindbridge_queue_service/mindbridge_queue_service

[Install]
WantedBy=multi-user.target

